---
import BaseLayout from "../layouts/baseLayout.astro";
import IconThing from "../components/iconThing.astro"
import ProjCard from "../components/projCard.astro";
import ExperienceCard from "../components/experienceCard.astro"
const props = Astro.props
---

<BaseLayout title="Homepage">
  <div class="font-mono">
    <section
      class="border-2 border-greyish p-5 inset-shadow-sm inset-shadow-gray-900/50 rounded-2xl bg-greyish"
    >
      <div class="text-3xl">Hello, I am</div>
      <div class="py-5 text-5xl text-bluu">Nicole Chuah</div>
      <div class="text-3xl">A student studying Information Technology</div>
      <ul class = "text-md mt-10 text-bluu">
        <li>One day.. Maybe...</li>
        <li>A full stack developer (?)</li>
        <li>A data engineer (?)</li>
        
      </ul>
    </section>

    <section>
      <div class="text-3xl pl-5 pt-12">Projects </div>
      <div class = "grid grid-cols-1 md:grid-cols-2 gap-2">
    <ProjCard 
    title="Personal Website"
    description="Hmm I am not sure but you are looking at it now"
    aUrl="https://github.com/smollcoin/coin"
    link="Github"
  >
        <div slot ="icon">
        <IconThing>
          <svg
          xmlns="http://www.w3.org/2000/svg"
          width="1em"
          height="1em"
          viewBox="0 0 32 32"
